$PAGE_HEADER_HEIGHT : 10%;

@import "fonts";
@import "animations";
@import "mixins";


div { padding:0; margin:0; }

body {
    -webkit-touch-callout: none;                /* prevent callout to copy image, etc when tap to hold */
    -webkit-text-size-adjust: none;             /* prevent webkit from resizing text to fit */
    -webkit-user-select: none;                  /* prevent copy paste, to allow, change 'none' to 'text' */
    background-color:#2a3052;
    font-family:proxima-nova, arial, helvetica, sans-serif;
    font-size:12px;
    height:100%;
    margin:0;
    padding:0;
    width:100%;
    
    >iframe { position:absolute; width:100%; height:100%; border:0; padding:0; margin:0; z-index:9999999; }
    
    >.wrapper {
        
        >div {
            visibility:hidden; z-index:-1;
            position:absolute;
            left:0;
            right:0;
            height:100%;
            width:100%;
            background-color:#2a3052;
        }
        
        >.loading {
            position:fixed;
            left:0;
            right:0;
            height:100%;
            width:100%;
            text-align:center;
            >.wrapper {
                position:relative;
                height:100%;

                .progress {
                    >.wrapper {
                        position:absolute;
                        left:25%;
                        top:40%;
                        width:50%;
                        border:1px solid #fff;
                        padding:5%;
                        >.percent {
                            height:50%; background-color:#fff;
                        }
                    }
                }
                @media screen and (min-aspect-ratio: 1/1) {
                    >.logo { margin:1% 0 1% 0; }
                   
                } 
            }
        }
                
        >.main {
            
            position:fixed;
            
            >.wrapper {
                
                position:relative;
                height:100%;
                margin:0; padding:0; border:0;
                
                >.logo {
                    height:17%;
                    background: url("../img/logo.svg") no-repeat;
                    background-size:80% 80%;
                    background-position:50% 100%;
                    margin:0; padding:0; border:0;
                }

                >.menu {
                    @include pagemenu;
                    .trade {
                        .wrapper {
                            background: url("../img/main/trade.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .charts {
                        .wrapper {
                            background: url("../img/main/charts.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .portfolio {
                        .wrapper {
                            background: url("../img/main/portfolio.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .support {
                        .wrapper {
                            background: url("../img/main/support.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .news {
                        .wrapper {
                            background: url("../img/main/news.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .settings {
                        .wrapper {
                            background: url("../img/main/settings.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                }
                
                //@media screen and (min-aspect-ratio: 1/1) {
                   // >.logo { margin:1% 0 1% 0; }
                //}
            }
        }
            
        >.support {
            height:100%;
            position:fixed;
            .wrapper {
                margin:0; padding:0; border:0; height:100%;
                >.header {
                    @include pageheader;
                    >.title {
                        background: url("../img/support/support.svg") no-repeat -23px 12px;
                        background-size:60% 60%;
                    }
                }
                >.menu {
                    @include pagemenu;
                    .faq {
                        .wrapper {
                            background: url("../img/support/faq.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .email {
                        .wrapper {
                            background: url("../img/support/email.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .chat {
                        .wrapper {
                            background: url("../img/support/chat.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .phone {
                        .wrapper {
                            background: url("../img/support/phone.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .callback {
                        .wrapper {
                            background: url("../img/support/callback.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .messages {
                        .wrapper {
                            background: url("../img/support/messages.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                }
            }
        }
        
        >.portfolio {
            height:100%;
            position:fixed;
            .wrapper {
                margin:0; padding:0; border:0; height:100%;
                >.header {
                    @include pageheader;
                    >.title {
                        background: url("../img/portfolio/portfolio.svg") no-repeat 0 45%;
                        background-size:80% 80%;
                    }
                }
                >.menu {
                    @include pagemenu;

                }
            }
        }
        
        >.news {
            height:100%;
            position:fixed;
            .wrapper {
                margin:0; padding:0; border:0; height:100%;
                >.header {
                    @include pageheader;
                    >.title {
                        background: url("../img/news/news.svg");
                        background-size:90% 90%;
                    }
                }
                >.menu {
                    @include pagemenu;
                    .markets {
                        .wrapper {
                            background: url("../img/news/markets.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .service {
                        .wrapper {
                            background: url("../img/news/service.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                }
            }
        }
        
        >.settings {
            height:100%;
            position:fixed;
            .wrapper {
                margin:0; padding:0; border:0; height:100%;
                >.header {
                    @include pageheader;
                    >.title {
                        background: url("../img/settings/settings.svg");
                        background-size:90% 90%;
                    }
                }
                >.menu {
                    @include pagemenu;
                    .locale {
                        .wrapper {
                            background: url("../img/settings/locale.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                    .debug {
                        .wrapper {
                           
                        }
                        .on {
                            background: url("../img/settings/debug/on.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                        .off {
                            background: url("../img/settings/debug/off.svg") #fff no-repeat 50% 80%;
                            background-size:70% 70%;
                        }
                    }
                }
            }
        } 
        
        >.charts {
            height:100%;
            position:fixed;
            .wrapper {
                margin:0; padding:0; border:0; height:100%;
                >.header {
                    @include pageheader;
                    >.title {
                        background: url("../img/charts/charts.svg");
                        background-size:60% 60%;
                    }
                }
                >.menu {
                    @include pagemenu;

                }
            }
        } 
        
        >.trade {
            height:100%;
            position:fixed;
            .wrapper {
                height:100%;
                
                .header {
                    @include pageheader;
                    >.title {
                        background: url("../img/trade/trade.svg") no-repeat;
                        background-size:90% 90%;
                    }
                }
                
                .content {
                    height:85%;
                    width:100%;
                    padding:0;
                    margin:0;
                    .markets, .symbols {
                        height:100%;
                        width:46%;
                        overflow:auto;
                        float:left;
                        margin:0;
                        padding:0;
                        > div {
                            margin:0 5% 8% 5%;
                            float:left;
                            padding:3%;
                            background:#fff;
                            font-size:20px;
                            border: 2px solid #FAFAFA;
                            border-radius:5px;
                            &:hover { cursor:pointer; background:#ea8125 }
                        }
                    }
                    
                    .markets {
                        margin-right:2%;
                        width:43%; 
                        border-right: 2px solid #FAFAFA;
                        padding:0 5% 0 2%;
                        
                        > div.selected {
                            @include objselected;
                        }
                        
                    }
                    
                }
                
                /* 
                @media screen and (max-aspect-ratio: 1/1) {
                    
                    .header {
                        height:65px;
                         >.back {
                            width:70px;   
                         }
                    }
                    
                    
                    .content {
                        
                        .markets, .types {
                            
                            div {
                                float:left;
                                font-size:16px;
                                padding:12px 10px 12px 10px;
                                border-radius:10px;
                            }
                          
                        }

                    }
                        
                        
                } */
                
                
                
            }

        }

    }
    
    >.status {
    position:fixed;
    background:url('../app/plugins/status/close.svg') #676d8d no-repeat;
    background-size: 30px 30px;
    background-position: right 10px top 10px;
    width:100%;
    z-index:999999999;
    bottom:0;
    left:0;
    color:#000;
    font-size:120%;
    border-top: solid 3px #fff;
    display:none;
    nav {
      position:relative;
      padding:12px 50px 20px 20px;
      ul {
	margin:0;
	padding:0;
	li {
	-webkit-animation-name: fadeInRightBig;
	-moz-animation-name: fadeInRightBig;
	-o-animation-name: fadeInRightBig;
	animation-name: fadeInRightBig;
	-webkit-animation-duration:1s;
	-moz-animation-duration:1s;
	-ms-animation-duration:1s;
	-o-animation-duration:1s;
	animation-duration:1s;
	  :last-child { margin-right:0; }
	  list-style:none;
	  border-radius:8px;
	  border:4px solid #fff;
	  margin:12px 12px 0 0;
	  padding:12px 20px 12px 12px;
	  white-space:nowrap;
	  display:inline-block;
	  background: #fff;
          .title { font-weight:bold; }
	  .type {
	    display:table-cell;
	    padding-right:6px;
	    div { width:24px; height:24px; background-size: 24px 24px; }
	    .warn { background:url('../app/plugins/status/type/warn.png') no-repeat; }
	    .info { background:url('../app/plugins/status/type/info.png') no-repeat; }
	    .critical { background:url('../app/plugins/status/type/critical.png') no-repeat; }
	  }
	  .details {
	    display:table-cell;
	    vertical-align:middle;
	    div {
	      white-space:nowrap; display:block;
	      .code {
		margin:0; display:inline; font-weight:bold;
		&:after { content:'-'; padding:0 3pt 0 3pt; }
	      }
	    }
	  }
	}
      }
    }
  }
  
  form {
    
    input:focus, select:focus, textarea:focus {
      outline: none;
    }
    
    >.apmessage {
      display:none;
      position:absolute;
      line-height:1;
      border-radius:1px;
      margin:2px 0 0 1px;
      white-space:nowrap;
      font-size:12px;
      background-color:#fff496;
      color:#111111;
      padding:2px 3px 2px 3px;
      z-index:99999;
    }
  }

}



/* app css */
.ajax {
  position:relative;
  div {
    position:absolute;
    z-index:99999999;
    width: 1.7em; height: 1.7em;
    left:50%;
    top:5px;
    border: solid #fff 1em;
    border-right-color: transparent;
    border-radius: 50%;
    -webkit-animation: spin 1s linear infinite; -moz-animation: spin 1s linear infinite;
    -ms-animation: spin 1s linear infinite; -o-animation: spin 1s linear infinite;
    animation: spin 1s linear infinite;
  }
}


